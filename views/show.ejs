<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous" />

    
    <div class="container">
        <div class="row row-cols-2 row-cols-lg-4 g-2 g-lg-3">
            <% data.docs.forEach(function(docs){ %>
          <div class="p-3 col">
            <div class=" p-3 card" style="width: 16rem;">
                <img src="http://localhost:3000/<%- docs.coverImage %>" class="card-img-top" height='220';width='115'alt="...">
                <div class="card-body">
                  <h5 class="card-title text-danger"><%- docs.name %></h5>
                  <h5 class="card-title">Price : <%- docs.price %></h5>
                  <% if(data.user) {%>
                  <a href="/books/delete?id=<%= docs.id%>" class="btn btn-primary">DELETE</a>
                  <% } %>
                  <% if(!data.user) { %>
                    <a href="#" class="btn btn-primary">BUY</a>
                  <% } %>
                </div>
              </div>
          </div>
          <%})%>
        </div>
      </div>
